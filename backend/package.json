{
  "name": "softwareproject-backend",
  "version": "1.0.0",
  "description": "Template for the softwareproject lecture at the University of Konstanz, Germany",
  "private": true,
  "scripts": {
    "build": "tsc && tsc --noEmit && eslint \"**/*.{js,ts}\"",
    "start": "node dist/server.js",
    "lint": "eslint \"**/*.{js,ts}\"",
    "test": "jest --config jest.config.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"tsc -w\" \"nodemon --watch dist/*\""
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "2.8.5",
    "errorhandler": "1.5.1",
    "express": "4.19.2",
    "express-validator": "^7.0.1",
    "pg": "8.11.4",
    "sequelize": "6.37.2",
    "source-map-support": "0.5.21",
    "uid-safe": "^2.1.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "2.8.17",
    "@types/errorhandler": "1.5.3",
    "@types/eslint": "8.56.7",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/source-map-support": "0.5.10",
    "@types/supertest": "6.0.2",
    "@types/uid-safe": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jest": "29.7.0",
    "nodemon": "3.1.0",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "typescript": "5.4.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
